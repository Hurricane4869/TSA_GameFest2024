# This .gitignore file should be placed at the root of your Unity project directory

# Ignore all .tmp files
*.tmp

# Ignore temporary build files
Temp/
Obj/
Build/

# Ignore auto-generated asset files
Library/
Assets/AssetStoreTools*
*.csproj
*.unityproj
*.sln
*.suo
*.user
*.userprefs
*.pidb
*.booproj
*.svd

# Ignore log files
*.log

# Ignore crash reports
sysinfo.txt

# Ignore memory capture files
*.mem

# Ignore asset files generated by version control
*.pidb.meta
*.pidb

# Ignore compiled assemblies
*.dll
*.exe
*.pdb
*.ilk
*.ipdb
*.iobj
*.a
*.lib

# Ignore the main build output folder (optional)
Builds/

# Ignore any crash reports, system info or stack traces generated by Unity
*.crash
*.stacktrace

# Ignore the AutoBuild folder (optional)
AutoBuild/

# Ignore Unity3D-generated files
Unity3D/
